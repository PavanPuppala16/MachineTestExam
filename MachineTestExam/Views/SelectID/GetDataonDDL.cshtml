@using MachineTestExam.Models;

@{
    ViewData["Title"] = "GetDataonDDL";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




@using (Html.BeginForm())
{
    if (ViewData["Val"] == null)
    {
        @Html.DropDownList("test", new SelectList(ViewBag.data, "UserId", "UserId"), "Please select", new { @id = "ddlFruits" })
        <input type="submit" value="submit" />
    }
    else
    {
        @Html.DropDownList("test", new SelectList(ViewBag.data, "UserId", "UserId"), "Please select", new { @id = "ddlFruits" })
        <input type="submit" value="submit" />
        <table class="table table-striped">
            <tr>
                <th>UserId</th>
                <th>UserName</th>
                <th>EmailId </th>
                <th>Password </th>
                <th>Branch </th>
                <th>PhoneNo </th>
                <th>IDProof</th>
                <th>IDno</th>
                <th>NoID</th>
                <th>JoiningDate</th>
                <th>CalculateDate</th>
               
            </tr>
            @foreach (var item in ViewData["Val"] as List<MachineTestExam.Models.Registration>)
            {
                <tr>
                    <td>@item.UserId</td>
                    <td>@item.UserName</td>
                    <td>@item.EmailId</td>
                    <td>@item.Password</td>
                    <td>@item.Branch</td>
                    <td>@item.PhoneNo</td>
                    <td>@item.IDProof</td>
                    <td>@item.IDno</td>
                    <td>@item.NoID</td>
                    <td>@item.JoiningDate</td>
                    <td>@item.CalculateDate</td>
                   
                </tr>
            }
        </table>
    }
}






























